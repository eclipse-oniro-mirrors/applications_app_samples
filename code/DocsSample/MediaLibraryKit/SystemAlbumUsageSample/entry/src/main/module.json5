{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "default"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "ohos.want.action.home"
            ]
          }
        ]
      },
      {
        "name": "GetFavoriteObjectAbility",
        "srcEntry": "./ets/getfavoriteobjectability/GetFavoriteObjectAbility.ets",
        "description": "$string:GetFavoriteObjectAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:GetFavoriteObjectAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "GetMediaFromFavoritesAbility",
        "srcEntry": "./ets/getmediafromfavoritesability/GetMediaFromFavoritesAbility.ets",
        "description": "$string:GetMediaFromFavoritesAbilitiy_desc",
        "icon": "$media:layered_image",
        "label": "$string:GetMediaFromFavoritesAbilitiy_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "GetVideoAlbumAbility",
        "srcEntry": "./ets/getvideoalbumability/GetVideoAlbumAbility.ets",
        "description": "$string:GetVideoAlbumAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:GetVideoAlbumAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "GetVideosFromVideoAlbumAbility",
        "srcEntry": "./ets/getvideosfromvideoalbumability/GetVideosFromVideoAlbumAbility.ets",
        "description": "$string:GetVideosFromVideoAlbumAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:GetVideosFromVideoAlbumAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      }
    ],
    "extensionAbilities": [
      {
        "name": "EntryBackupAbility",
        "srcEntry": "./ets/entrybackupability/EntryBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ]
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.READ_IMAGEVIDEO",
        "reason": "$string:read_imagevideo",
        "usedScene": {
          "abilities": [
            "TestAbility",
            "GetFavoriteObjectAbility",
            "GetMediaFromFavoritesAbility",
            "GetVideoAlbumAbility",
            "GetVideosFromVideoAlbumAbility"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.WRITE_IMAGEVIDEO",
        "reason": "$string:write_imagevideo",
        "usedScene": {
          "abilities": [
            "TestAbility",
            "GetFavoriteObjectAbility",
            "GetMediaFromFavoritesAbility",
            "GetVideoAlbumAbility",
            "GetVideosFromVideoAlbumAbility"
          ],
          "when": "always"
        }
      }
    ]
  }
}