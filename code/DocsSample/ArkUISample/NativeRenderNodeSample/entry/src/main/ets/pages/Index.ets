import router from '@system.router';
import entry from 'libentry.so';

@Entry
@Component
struct Index {

  build() {
    Flex() {
      Column() {
        Button("RenderNode 节点操作demo").onClick(() => {
          router.push({
            uri: 'pages/entry'
          });
        })
        Button("RenderNode 属性设置demo").onClick(() => {
          router.push({
            uri: 'pages/property'
          });
        })
        Button("RenderNode 自定义绘制demo").onClick(() => {
          router.push({
            uri: 'pages/custom'
          });
        })
      }.width('100%')
    }.width('100%')
  }
}
