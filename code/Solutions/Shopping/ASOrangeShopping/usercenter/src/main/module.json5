{
  "module": {
    "name": "usercenter",
    "type": "feature",
    "description": "$string:usercenter_desc",
    "mainElement": "UserCenterAbility",
    "deviceTypes": [
      "default",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": true,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "UserCenterAbility",
        "srcEntrance": "./ets/usercenterability/UserCenterAbility.ets",
        "description": "$string:MainAbility_desc",
        "icon": "$media:user_center_icon",
        "label": "$string:MainAbility_label",
        "launchType": "specified",
        "startWindowIcon": "$media:user_center_icon",
        "startWindowBackground": "$color:transfer",
        "visible": true
      }
    ]
  }
}